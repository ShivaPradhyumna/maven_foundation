package streams;

public class sample4 {
	public static void main(String[] args) {
		int[] arr = {34,23,1,24,75,33,54,8};
		int a = arr[0];
		for(int i: arr)
		{
			if(i<a) a = i;
		}
		int k = 0;
		while(a>0)
		{
			k += a%10;
			a=a/10;
		}
		if(k%2!=0) System.out.println(0);
		else System.out.println(1);
	}
}
